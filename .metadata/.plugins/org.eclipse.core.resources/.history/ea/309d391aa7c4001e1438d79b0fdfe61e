package tree;

public class Runner {
	public static void main(String[] args) {
		Node root=new Node(20);
		BTSImpln b1=new BTSImpln();
		b1.insert(root, 10);
		b1.insert(root, 30);
		b1.insert(root, 5);
		b1.insert(root, 15);
		b1.insert(root, 35);
		b1.insert(root, 3);
		b1.insert(root, 23);
		b1.insert(root, 45);
		b1.insert(root, 34);
		b1.insert(root, 31);
	    b1.preOrder(root);
	    b1.postOrder(root);
	}
	
}
